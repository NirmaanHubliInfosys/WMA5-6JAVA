package Collection;

public class ArrayListDemo {

	public static void main(String[] args) {
		ArrayList al=new ArrayList<>();
		al.add("ashok");//index of 0
		al.add(101);//index of 1
		al.add(202.05);//index of 2
		System.out.println(al);
		al.remove(1);
		System.out.println(al);
		al.add("karnataka");
		al.add("java");
		al.add("java");
		al.add(null);
		System.out.println(al);

	}

}
