package oops;

public class Class_Bike2 {

	public static void main(String[] args) {
		 void run()
		 {
			 System.out.println("Bike is running safely");
			 
		 }  
		  
		  public static void main(String args[]){  
		  Bike2 obj = new Bike2();//creating object  
		  obj.run();//calling method  
		  }  
	}

}
